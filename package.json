{
  "name": "optimizely-fe-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next/cache",
    "dev": "npm run clean && next dev",
    "dev:custom": "dotenv -e .env/.env.custom.local -- pnpm dev",
    "dev:vb": "dotenv -e .env/.env.visual.local -- pnpm dev",
    "build:custom": "dotenv -e .env/.env.custom.local -- next build",
    "build:vb": "dotenv -e .env/.env.visual.local -- next build",
    "start": "next start",
    "lint": "next lint",
    "bdd": "cucumber-js --require-module ts-node/register --require features/step_definitions/**/*.ts",
    "test:bdd": "start-server-and-test dev http://localhost:3000 bdd",
    "test:features": "cucumber-js features/**/*.feature --require-module ts-node/register --require features/step-definitions/**/*.ts --require features/support/**/*.ts",
    "codegen:custom": "dotenv -e .env/.env.custom.local -- graphql-codegen --config scripts/codegen.custom.ts",
    "codegen:vb": "dotenv -e .env/.env.visual.local -- graphql-codegen --config scripts/codegen.visual.ts"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.525.0",
    "negotiator": "^1.0.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.2",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/env": "^15.3.5",
    "@remkoj/optimizely-graph-client": "^5.1.0",
    "@remkoj/optimizely-graph-functions": "^5.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/chai": "^5.2.2",
    "@types/graphql": "^14.5.0",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "chai": "^5.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "graphql": "^16.11.0",
    "graphql-request": "^6.1.0",
    "jiti": "^2.4.2",
    "openapi-to-graphql": "^3.0.7",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.12",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3"
  }
}
